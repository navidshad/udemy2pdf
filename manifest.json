{
	"name": "Udemy 2 pdf",
	"description": "Save udemy notes as a pdf",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"activeTab",
		"scripting"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.udemy.com/course/*/learn/lecture/*"
			],
			"run_at": "document_idle",
			"js": [
				"note-collector.js"
			]
		}
	]
}